{
  "name": "sections-gallery",
  "version": "1.0.0",
  "description": "Premium Shopify theme sections marketplace and management app",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "test": "jest",
    "lint": "eslint ."
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": [
    "shopify",
    "sections",
    "theme",
    "marketplace",
    "liquid"
  ],
  "author": "Sections Gallery Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@shopify/shopify-api": "^8.1.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "stripe": "^14.7.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
{
  "name": "sections-gallery",
  "version": "2.0.0",
  "description": "Premium Shopify section marketplace with Shopify Billing API integration",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run admin\"",
    "server": "nodemon server.js",
    "client": "cd frontend && npm start",
    "admin": "cd admin && npm start",
    "build": "npm run build:client && npm run build:admin",
    "build:client": "cd frontend && npm run build",
    "build:admin": "cd admin && npm run build",
    "start": "node server.js",
    "migrate": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "seed": "node prisma/seed.js",
    "studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:push": "npx prisma db push",
    "postinstall": "npx prisma generate"
  },
  "keywords": [
    "shopify",
    "sections",
    "marketplace",
    "saas",
    "liquid",
    "themes",
    "ecommerce"
  ],
  "author": {
    "name": "Shafran Mohamed",
    "email": "safranlive@gmail.com",
    "company": "SYSTO",
    "url": "https://www.systo.lk"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Safranlive/sections-gallery.git"
  },
  "bugs": {
    "url": "https://github.com/Safranlive/sections-gallery/issues",
    "email": "safranlive@gmail.com"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@shopify/shopify-api": "^9.0.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@shopify/polaris": "^12.0.0",
    "@shopify/polaris-icons": "^7.0.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  }
}